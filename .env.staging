# Staging Environment Variables for Checkmate App

# Plaid Configuration (Development/Sandbox)
EXPO_PUBLIC_PLAID_CLIENT_ID=your_sandbox_plaid_client_id
EXPO_PUBLIC_PLAID_SECRET=your_sandbox_plaid_secret
EXPO_PUBLIC_PLAID_ENVIRONMENT=sandbox

# App Configuration
EXPO_PUBLIC_APP_ENV=staging
EXPO_PUBLIC_API_BASE_URL=https://sandbox.plaid.com

# Analytics and Monitoring (Optional)
EXPO_PUBLIC_SENTRY_DSN=your_staging_sentry_dsn
EXPO_PUBLIC_ANALYTICS_KEY=your_staging_analytics_key

# Security Settings
EXPO_PUBLIC_DEBUG_MODE=true